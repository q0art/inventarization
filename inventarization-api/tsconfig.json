{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app": ["./src/app.module.ts"],
      "@app/shared/*": ["libs/shared/src/*"],
      //
      "@auth/*": ["./src/auth/*"],
      "@brand/*": ["./src/brand/*"],
      "@case/*": ["./src/case/*"],
      "@cooler/*": ["./src/cooler/*"],
      "@cpu/*": ["src/cpu/*"],
      "@departament/*": ["./src/departament/*"],
      "@desktop/*": ["./src/desktop/*"],
      "@employee/*": ["./src/employee/*"],
      "@gpu/*": ["./src/gpu/*"],
      "@headphone/*": ["./src/headphone/*"],
      "@keyboard/*": ["./src/keyboard/*"],
      "@laptop/*": ["./src/laptop/*"],
      "@monitor/*": ["./src/monitor/*"],
      "@motherboard/*": ["./src/motherboard/*"],
      "@mouse/*": ["./src/mouse/*"],
      "@mousepad/*": ["./src/mousepad/*"],
      "@prisma/*": ["./src/prisma/*"],
      "@ram/*": ["./src/ram/*"],
      "@ssd/*": ["./src/ssd/*"],
      "@user/*": ["./src/user/*"],
      "@workspace/*": ["./src/workspace/*"]
    }
  }
}
